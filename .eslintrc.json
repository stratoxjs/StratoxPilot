{
  "extends": ["airbnb-base"], 
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "rules": {
    "no-unused-vars": "off",
    "class-methods-use-this": "off",
    "no-console": "off",
    "no-plusplus": "off"
  }
}
